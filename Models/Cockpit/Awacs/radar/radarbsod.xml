<?xml version="1.0"?>

<PropertyList>

	<path>radarbsod.ac</path>
	
<!-- Power on/off -->
	<animation>
		<name>Power on/off</name>
		<type>select</type>
		<object-name>Off</object-name>
		<condition>
			<or>
				<less-than>
					<property>b707/ess-bus</property>
					<value>20</value>
				</less-than>
				<not><property>instrumentation/mptcas/on</property></not>
			</or>
		</condition>
	</animation>
	
</PropertyList>
