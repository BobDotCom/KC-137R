<?xml version="1.0"?>

<PropertyList>

	<path>radarbknd.ac</path>
	
<!-- Power on/off -->
	<animation>
		<name>Power on/off</name>
		<type>select</type>
		<object-name>Off</object-name>
		<condition>
			<less-than>
				<property>systems/electrical/outputs/efis</property>
				<value>15</value>
			</less-than>
		</condition>
	</animation>
	
</PropertyList>
