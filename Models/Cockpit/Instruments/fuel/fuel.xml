<?xml version="1.0"?>
<PropertyList>

<path>fuel.ac</path>

<animation>
  <type>material</type>
  <object-name>FuelT1</object-name>
  <object-name>FuelT2</object-name>
  <object-name>FuelT3</object-name>
  <object-name>FuelT4</object-name>
  <object-name>FuelT5</object-name>
  <emission>
    <red>1</red>
    <green>1</green>
    <blue>1</blue>
