<?xml version="1.0"?>

<PropertyList>

	<path>fbw.ac</path>

	<animation>
		<type>material</type>
		<object-name>LampAmber1</object-name>
		<object-name>LampAmber2</object-name>
		<object-name>switch1</object-name>
		<object-name>switch2</object-name>
		<object-name>sockle1</object-name>
		<object-name>sockle2</object-name>
		<condition>
			<less-than>
				<property>systems/electrical/outputs/cabin-dim</property>
				<value>0.8</value>
			</less-than>
		</condition>
		<emission>
			<red>0.15</red>
			<green>0.15</green>
			<blue>0.15</blue>
			<factor-prop>/systems/electrical/outputs/panel-dim</factor-prop>
		</emission>
	</animation>

	<animation>
		<type>material</type>
		<object-name>LampAmber1</object-name>
		<object-name>LampAmber2</object-name>
		<object-name>switch1</object-name>
		<object-name>switch2</object-name>
		<object-name>sockle1</object-name>
		<object-name>sockle2</object-name>
		<object-name>sup</object-name>
		<object-name>shadow</object-name>
		<condition>
			<not><property>sim/rendering/rembrandt/enabled</property></not>
			<property>systems/electrical/outputs/cabin-dim</property>
		</condition>
		<emission>
			<red>0.25</red>
			<green>0.25</green>
			<blue>0.25</blue>
			<factor-prop>/systems/electrical/outputs/cabin-dim</factor-prop>
		</emission>
	</animation>	

	<animation>
		<type>noshadow</type>
		<object-name>LampAmber1</object-name>
		<object-name>LampAmber2</object-name>
		<object-name>LampAmberON1</object-name>
		<object-name>LampAmberON2</object-name>
	</animation>

	<effect>
		<inherits-from>Effects/model-transparent</inherits-from>
		<object-name>shadow</object-name>
	</effect>

	<animation>
		<type>material</type>
		<object-name>LampAmberON1</object-name>
		<object-name>LampAmberON2</object-name>
		<emission>
			<red>1.0</red>
			<green>1.0</green>
			<blue>0.5</blue>
		</emission>
	</animation>

	<animation>
		<type>select</type>
		<object-name>LampAmberON1</object-name>
		<condition>
			<greater-than>
				<property>b707/ess-bus</property>
				<value>20</value>
			</greater-than>
			<or>
				<and>
					<property>it-fbw/roll-disable</property>
				</and>
				<property>controls/test/warn-knob</property>
			</or>
		</condition>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>switch1</object-name>
		<property>it-fbw/man-roll-btn</property>
		<factor>60</factor>
		<offset-deg>-60</offset-deg>
		<center>
			<x-m>0.00113</x-m>
			<y-m>0.0</y-m>
			<z-m>-0.00092</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>switch1</object-name>
		<object-name>sockle1</object-name>
		<visible>true</visible>
		<action>
			<button>0</button>
			<repeatable>true</repeatable>
			<binding>
				<command>nasal</command>
				<script>fbw.roll();</script>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>LampAmberON2</object-name>
		<condition>
			<greater-than>
				<property>b707/ess-bus</property>
				<value>20</value>
			</greater-than>
			<or>
				<and>
					<property>it-fbw/pitch-disable</property>
				</and>
				<property>controls/test/warn-knob</property>
			</or>
		</condition>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>switch2</object-name>
		<property>it-fbw/man-pitch-btn</property>
		<factor>60</factor>
		<offset-deg>-60</offset-deg>
		<center>
			<x-m>0.00113</x-m>
			<y-m>0.0</y-m>
			<z-m>-0.00092</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>pick</type>
		<object-name>switch2</object-name>
		<object-name>sockle2</object-name>
		<visible>true</visible>
		<action>
			<button>0</button>
			<repeatable>true</repeatable>
			<binding>
				<command>nasal</command>
				<script>fbw.pitch();</script>
			</binding>
		</action>
	</animation>

</PropertyList>

