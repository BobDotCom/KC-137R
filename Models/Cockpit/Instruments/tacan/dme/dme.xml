<?xml version="1.0"?>

<!-- If you are reading this, I give you a pie. -->

<PropertyList>

	<name>dme</name>
	<path>dme.ac</path>
	
	<animation>
		<type>select</type>
		<object-name>octal</object-name>
		<object-name>ident</object-name>
		<object-name>dme</object-name>
		<object-name>freqs</object-name>
		<object-name>freq-tacan</object-name>
		<object-name>freq-normal</object-name>
		<condition>
			<greater-than-equals>
				<property>systems/electrical/outputs/efis</property>
				<value>15</value>
			</greater-than-equals>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>XX1</object-name>
		<condition>
			<and>
				<greater-than-equals>
					<property>systems/electrical/outputs/efis</property>
					<value>15</value>
				</greater-than-equals>
				<equals>
					<property>/instrumentation/tacan/powered</property>
					<value>0</value>
				</equals>
			</and>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>freq-tacan</object-name>
		<object-name>freq-normal</object-name>
		<condition>
			<and>
				<greater-than-equals>
					<property>systems/electrical/outputs/efis</property>
					<value>15</value>
				</greater-than-equals>
				<equals>
					<property>/instrumentation/tacan/powered</property>
					<value>1</value>
				</equals>
			</and>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>XX2</object-name>
		<condition>
			<and>
				<greater-than-equals>
					<property>systems/electrical/outputs/efis</property>
					<value>15</value>
				</greater-than-equals>
				<or>
					<equals>
						<property>/instrumentation/tacan/in-range</property>
						<value>0</value>
					</equals>
					<equals>
						<property>/instrumentation/tacan/powered</property>
						<value>0</value>
					</equals>
				</or>
			</and>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>ident2</object-name>
		<object-name>dme2</object-name>
		<condition>
			<and>
				<greater-than-equals>
					<property>systems/electrical/outputs/efis</property>
					<value>15</value>
				</greater-than-equals>
				<equals>
					<property>/instrumentation/tacan/in-range</property>
					<value>1</value>
				</equals>
				<equals>
					<property>/instrumentation/tacan/powered</property>
					<value>1</value>
				</equals>
			</and>
		</condition>
	</animation>
	
